<% if(message.length > 0) { %>
<div class="alert alert-<%= type %> alert-dismissible fade show" role="alert">
  <strong><span><%= message %></span></strong>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %>
